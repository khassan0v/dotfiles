  name = Alisher A. Khassanov
  email = a.khssnv@gmail.com
[core]
  autocrlf = input
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %ae | %s%d\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  first = rev-list --max-parents=0 HEAD
